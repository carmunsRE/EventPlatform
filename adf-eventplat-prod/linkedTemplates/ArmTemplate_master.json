{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-eventplat-prod"
		},
		"ls_sqldb_repes_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sqldb_repes'"
		},
		"ls_sqldb_toptal_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sqldb_toptal'"
		},
		"ls_sqldb_toptal_copy_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sqldb_toptal_copy'"
		},
		"ls_adl_toptaleventplat_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlstoptaleventplat.dfs.core.windows.net"
		},
		"ls_kv_toptaleventplat_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-toptaleventplat.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-eventplat-prod_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_sqldb_repes_connectionString": {
						"value": "[parameters('ls_sqldb_repes_connectionString')]"
					},
					"ls_sqldb_toptal_connectionString": {
						"value": "[parameters('ls_sqldb_toptal_connectionString')]"
					},
					"ls_sqldb_toptal_copy_connectionString": {
						"value": "[parameters('ls_sqldb_toptal_copy_connectionString')]"
					},
					"ls_kv_toptaleventplat_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_kv_toptaleventplat_properties_typeProperties_baseUrl')]"
					},
					"ls_adl_toptaleventplat_properties_typeProperties_url": {
						"value": "[parameters('ls_adl_toptaleventplat_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-eventplat-prod_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-eventplat-prod_ArmTemplate_0')]"
			]
		},
		{
			"name": "adf-eventplat-prod_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-eventplat-prod_ArmTemplate_1')]"
			]
		}
	]
}